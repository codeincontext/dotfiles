<?xml version="1.0"?>
<root>
  <item>
    <name>Map COMMAND_R to neovim escape</name>
    <identifier>private.neovim_escape</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyToKey__
      KeyCode::COMMAND_R,
      KeyCode::BACKSLASH, ModifierFlag::CONTROL_L,
      KeyCode::N, ModifierFlag::CONTROL_L
    </autogen>
  </item>

  <item>
    <name>Sculpt</name>
    <item>
      <name>F1 to Play/Pause</name>
      <identifier>private.sculpt.f1</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F1,
        ConsumerKeyCode::MUSIC_PLAY
      </autogen>
    </item>
    <item>
      <name>F2 to Mute</name>
      <identifier>private.sculpt.f2</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F2,
        ConsumerKeyCode::VOLUME_MUTE
      </autogen>
    </item>
    <item>
      <name>F3 to Volume Down</name>
      <identifier>private.sculpt.f3</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F3,
        ConsumerKeyCode::VOLUME_DOWN
      </autogen>
    </item>
    <item>
      <name>F4 to Volume Up</name>
      <identifier>private.sculpt.f4</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F4,
        ConsumerKeyCode::VOLUME_UP
      </autogen>
    </item>
    <item>
      <name>F5 to Previous Track</name>
      <identifier>private.sculpt.f5</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F5,
        ConsumerKeyCode::MUSIC_PREV
      </autogen>
    </item>
    <item>
      <name>F6 to Next Track</name>
      <identifier>private.sculpt.f6</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F6,
        ConsumerKeyCode::MUSIC_NEXT
      </autogen>
    </item>

    <item>
      <name>F7 to Previous Space</name>
      <identifier>private.sculpt.f7</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F7,
        KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L
      </autogen>
    </item>

    <item>
      <name>F8 to Next Space</name>
      <identifier>private.sculpt.f8</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F8,
        KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L
      </autogen>
    </item>
  </item>

</root>
